{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-pdo_sqlite": "^8.0",
        "doctrine/dbal": "^3.3",
        "fruitcake/laravel-cors": "^2.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/mail": "^9.1",
        "illuminate/notifications": "^9.1",
        "illuminate/redis": "^9.17",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "^9.0",
        "laravel/socialite": "^5.5",
        "league/flysystem": "^3.0",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.5",
        "predis/predis": "^1.1",
        "starlight93/html-pdf-excel": "^1.0",
        "starlight93/oauth2": "^1.2",
        "staudenmeir/laravel-cte": "^1.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^5.2",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/GlobalHelper.php",
            "app/Helpers/DBS.php",
            "app/Helpers/PLSQL.php",
            "app/Helpers/Logger.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
