{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2|^8.0",
        "cpliakas/git-wrapper": "^3.0",
        "doctrine/dbal": "^3.0",
        "flipbox/lumen-generator": "^8.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/mail": "^8.27",
        "illuminate/notifications": "^8.36",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "^8.0",
        "maatwebsite/excel": "^3.1",
        "starlight93/oauth2": "^1.0",
        "staudenmeir/laravel-cte": "^1.2",
        "stevebauman/location": "^5.0",
        "kitloong/laravel-migrations-generator": "^4.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "league/flysystem": "^1.1",
        "mockery/mockery": "^1.3.1",
        "overtrue/phplint": "^2.3",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/GlobalHelper.php",
            "app/Helpers/DBS.php",
            "app/Helpers/PLSQL.php",
            "app/Helpers/Logger.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
